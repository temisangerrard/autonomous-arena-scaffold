{
  "name": "@arena/web",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "prebuild": "npm run -w @arena/shared build",
    "build": "tsc -p tsconfig.json",
    "prestart": "npm run -w @arena/shared build",
    "start": "node dist/apps/web/src/server.js",
    "lint": "eslint src --ext .ts",
    "pretest": "npm run -w @arena/shared build",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@arena/shared": "0.1.0",
    "google-auth-library": "^9.15.1",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "redis": "^4.7.0"
  },
  "devDependencies": {
    "@types/pg": "^8.11.10",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2"
  }
}
